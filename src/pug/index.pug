extends /layout/_default-layout

prepend config-block
  - page_name = ''

//- append stylesheet-block

//- append javascript-block

block content-block
  -
    imageStore = [
      "1",
      "2",
      "special",
    ];
  .sidebar
    a.sidear__logo(href="") HIWAI
    .sidear__outline.js-outline-modal 免責事項
    each id, i in imageStore
      .sidebar__radio-box
        input.sidebar__radio.js-ossan-select(type="radio" name="ossan" id=id value=id)
        label.sidebar__label(for=id)
          img.sidebar__label-img(src=`img/ossan${id}-1.gif` checked=i=='0')
  .gmap.js-map
